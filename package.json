{
  "name": "quicklinks-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "formik": "^2.2.9",
    "npm-watch": "^0.11.0",
    "randomcolor": "^0.6.2",
    "react": "^17.0.2",
    "react-browser-extension-scripts": "4.0.10",
    "react-dom": "^17.0.2",
    "use-state-with-callback": "^2.0.3",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "npm run css && (SET INLINE_RUNTIME_CHUNK=false) && react-scripts start",
    "build": "npm run css && (SET INLINE_RUNTIME_CHUNK=false) && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "css": "postcss ./src/styles/tailwind.css -o ./src/styles/index.css",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "tailwindcss": "^2.2.19"
  }
}
